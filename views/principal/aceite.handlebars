<style>
    .disabledbutton {
        pointer-events: none;
        opacity: 0.4;
    }
</style>
<script>
    window.onload = function () {
        var bloquear = document.getElementById('bloquear')

        if (bloquear.className == 'true') {
            bloquear.className = 'disabledbutton'
        }

    }
</script>
<div style="width: 98%;padding-left: 2%;padding-top: 0.5%;">
    {{#if proposta._id}}
    <div style="width: 100%;">
        <h5 class="header">{{cliente_proposta.nome}} - {{proposta.seq}}</h5>
    </div>
    {{/if}}

    <div class='btn-group col-12'>
        <div class='numero'>1</div>
        <a href='/gerenciamento/proposta/{{proposta._id}}/'
            class='btn btn-sm {{#if proposta.feito}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center'
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-house d-flex align-items-center"></i>&nbspProposta</a>
        <div class='numero'>2</div>
        <a href="/gerenciamento/visita/{{proposta._id}}"
            class="btn btn-sm {{#if vistoria.feito}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-gear d-flex align-items-center"></i>&nbspVisita
            Técnica</a>
        <div class='numero '>3</div>
        <a href="/gerenciamento/assinatura/{{proposta._id}}"
            class="btn btn-sm {{#if proposta.assinado}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-pencil d-flex align-items-center"></i>&nbspContrato</a>
        <div class='numero'>4</div>
        <a href="/gerenciamento/compra/{{proposta._id}}"
            class="btn btn-sm {{#if compra.feitonota}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-cart-check d-flex align-items-center"></i>&nbspCompra Sistema</a>
        <div class='numero'>5</div>
        <a href="/gerenciamento/trt/{{proposta._id}}"
            class="btn btn-sm {{#if documento.feitotrt}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-tools d-flex align-items-center"></i>&nbspTRT</a>
        <div class='numero'>6</div>
        <a href="/gerenciamento/execucao/{{proposta._id}}"
            class="btn btn-sm {{#if documento.protocolado}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-paperclip d-flex align-items-center"></i>&nbspPlantas</a>
        <div class='numero'>7</div>
        <a href="/gerenciamento/equipe/{{proposta._id}}"
            class="btn btn-sm {{#if lista_equipe.feito}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-people d-flex align-items-center"></i>&nbspExecução</a>
        <div class='numero ativo'>8</div>
        <a href="/gerenciamento/aceite/{{proposta._id}}"
            class="btn btn-sm {{#if documento.feitoaceite}} check-menu {{else}} botao-menu ativo{{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-check2-square d-flex align-items-center"></i>&nbspAceite Obra</a>
        <div class='numero'>9</div>
        <a href="/gerenciamento/almoxarifado/{{proposta._id}}"
            class="btn btn-sm {{#if documento.feitoalmox}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-layers d-flex align-items-center"></i>&nbspAlmoxarifado</a>
        <div class='numero'>10</div>
        <a href="/gerenciamento/financeiro/{{proposta._id}}"
            class="btn btn-sm {{#if documento.feitofaturado}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-calculator d-flex align-items-center"></i>&nbspFinanceiro</a>
        <div class='numero'>11</div>
        <a href="/gerenciamento/posvenda/{{proposta._id}}"
            class="btn btn-sm {{#if posvenda.feito}} check-menu {{else}} botao-menu {{/if}} d-flex align-items-center"
            style="border-bottom-left-radius: 30px;">&nbsp&nbsp<i
                class="bi bi-door-open d-flex align-items-center"></i>&nbspPós
            Venda</a>
    </div>
    <div id='bloquear' class='{{proposta.encerrado}}'>
        <div class="row mt-2">
            <label class='col-form-label fw-bold'>Fotos</label>
            <div class="row">
                {{!-- <div class="col">
                    <div class="row" style="height: 380px;">
                        <object data="/gerenciamento/mostrarAceite/{{proposta._id}}" type="application/pdf">
                            <p>Seu navegador não tem um plugin pra PDF</p>
                        </object>
                    </div>
                </div> --}}
                <form action="/gerenciamento/caminhoAte" method="post" enctype="multipart/form-data">
                    <div class='row'>
                        <input type='hidden' name='id' value='{{proposta._id}}'>
                        <input type='hidden' name='aceite' value='true'>
                        <div class='col-md-3 text-end'>
                            <label class='col-form-label fw-bold'>Aterramento:</label>
                        </div>
                        <div class='col-md-9'>
                            <div class="row">
                                <div class="col">
                                    <input type='file' multiple name='fileate' class='form-control'>
                                </div>
                                {{#if atvate.caminhoFoto}}
                                <div class="col-md-1">
                                    <a href='/gerenciamento/mostrarAte/{{proposta._id}}' class='btn  btn-primary'><i
                                            class='bi bi-eye'></i></a>
                                </div>
                                {{/if}}
                                <div class="col-md-2">
                                    <div class="form-check mt-1">
                                        <input class="form-check-input" name='check' type="checkbox" {{checkAte}}>
                                        <label class="form-check-label mt-1" style="font-size: 15px;"
                                            for="flexCheckDefault">
                                            Aprovado
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <button type='submit' class='btn  btn-success'><i class='bi bi-save'></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <form action="/gerenciamento/caminhoInv" class='mt-4' method="post" enctype="multipart/form-data">
                    <div class='row'>
                        <input type='hidden' name='id' value='{{proposta._id}}'>
                        <input type='hidden' name='aceite' value='true'>
                        <div class="col-md-3 text-end">
                            <label class='col-form-label fw-bold'>Inversor e String Box:</label>
                        </div>
                        <div class="col-md-9">
                            <div class="row">
                                <div class="col">
                                    <input type='file' multiple name='fileinv' class='form-control'>
                                </div>
                                {{#if atvinv.caminhoFoto}}
                                <div class="col-md-1">
                                    <a href='/gerenciamento/mostrarInv/{{proposta._id}}' class='btn  btn-primary'><i
                                            class='bi bi-eye'></i></a>
                                </div>
                                {{/if}}
                                <div class="col-md-2">
                                    <div class="form-check mt-1">
                                        <input class="form-check-input" name='check' {{checkInv}} type="checkbox">
                                        <label class="form-check-label mt-1" style="font-size: 15px;">
                                            Aprovado
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <button type='submit' class='btn  btn-success'><i class='bi bi-save'></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <form action="/gerenciamento/caminhoMod" class='mt-4' method="post" enctype="multipart/form-data">
                    <div class="row">
                        <input type='hidden' name='id' value='{{proposta._id}}'>
                        <input type='hidden' name='aceite' value='true'>
                        <div class="col-md-3 text-end">
                            <label class='col-form-label fw-bold'>Estruturas e Módulos:</label>
                        </div>
                        <div class="col-md-9">
                            <div class="row">
                                <div class="col">
                                    <input type='file' multiple name='filemod' class='form-control'>
                                </div>
                                {{#if atvtel.caminhoFoto}}
                                <div class="col-md-1">
                                    <a href='/gerenciamento/mostrarMod/{{proposta._id}}' class='btn  btn-primary'><i
                                            class='bi bi-eye'></i></a>
                                </div>
                                {{/if}}
                                <div class="col-md-2">
                                    <div class="form-check mt-1">
                                        <input class="form-check-input" type="checkbox" name='check' {{checkMod}}>
                                        <label class="form-check-label mt-1" style="font-size: 15px;">
                                            Aprovado
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <button type='submit' class='btn  btn-success'><i class='bi bi-save'></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class='mt-4'>
                    <hr>
                </div>
                <label class='col-form-label fw-bold'>Documentos</label>
                <form action='/gerenciamento/checklist' class='' method='post' enctype="multipart/form-data">
                    <div class="row">
                        <div class="col-md-3 text-end">
                            <label class='col-form-label fw-bold mt-3'>Checklist de Instalação:</label>
                        </div>
                        <div class="col-md-9 text-start">
                            <input type='hidden' name='id' value='{{proposta._id}}'>
                            <div class="row">
                                {{#if documento.clins}}
                                <div class="col-md-5">
                                    <div class="row mt-3">
                                        <label class='col-form-label labelp'>{{documento.clins}}</label>
                                    </div>
                                </div>
                                <div class="col-md-1 mt-3">
                                    <a class="btn btn-primary" href='/gerenciamento/mostrarClins/{{proposta._id}}'
                                        target="_blank"><i class='bi bi-eye'></i></a>
                                </div>
                                <div class="col-md-2 mt-3">
                                    <input style="max-width: 150px;font-size: 15px;" type='file' class='form-control'
                                        name='clins' value='{{documento.clins}}'>
                                </div>
                                {{else}}
                                <div class="col-md-7 mt-3">
                                    <input type='file' class='form-control' name='clins' value='{{documento.clins}}'>
                                </div>
                                {{/if}}
                                <div style="width: 200px;">
                                    <label> Data</label>
                                    <input type='date' class='form-control' name='dtclins'
                                        value='{{documento.dtclins}}'>
                                </div>
                                <div class="col-md-1 mt-3">
                                    <button type='submit' class='btn btn-success'><i class='bi bi-save'></i></button>
                                </div>
                            </div><br>
                        </div>
                    </div>
                </form>
                <form action='/gerenciamento/aceite' class='' method='post' enctype="multipart/form-data">
                    <div class="row">
                        <div class="col-md-3 text-end">
                            <label class='col-form-label fw-bold mt-3'>Aceite da Instalação: </label>
                        </div>
                        <div class="col-md-9 text-start">
                            <input type='hidden' name='id' value='{{proposta._id}}'>
                            <div class="row">
                                {{#if documento.aceite}}
                                <div class="col-md-5">
                                    <div class="row mt-3">
                                        <label class='col-form-label labelp'>{{documento.aceite}}</label>
                                    </div>
                                </div>
                                <div class="col-md-1 mt-3">
                                    <a class="btn btn-primary" href='/gerenciamento/mostrarAceite/{{proposta._id}}'
                                        target="_blank"><i class='bi bi-eye'></i></a>
                                </div>
                                <div class="col-md-2 mt-3">
                                    <input style="max-width: 150px;font-size: 15px;" type='file' class='form-control'
                                        name='aceite' value='{{documento.aceite}}'>
                                </div>
                                {{else}}
                                <div class="col-md-7 mt-3">
                                    <input type='file' class='form-control form-control' name='aceite'
                                        value='{{documento.aceite}}'>
                                </div>
                                {{/if}}
                                <div style="width: 200px;">
                                    <label> Data</label>
                                    <input type='date' class='form-control form-control' name='dtaceite'
                                        value='{{documento.dtaceite}}'>
                                </div>
                                <div class="col-md-1 mt-3">
                                    <button type='submit' class='btn btn-success'><i class='bi bi-save'></i></button>
                                </div>
                            </div><br>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>