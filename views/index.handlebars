<script>
    function login() {
        window.location.href = '/usuario/login'
    }
    function registrar() {
        window.location.href = '/usuario/registrar/todos'
    }
    function changeBenefit(num) {
        var ben1 = document.getElementById('benefit-1')
        var descben1 = document.getElementById('desc-benefit-1')
        var ben2 = document.getElementById('benefit-2')
        var descben2 = document.getElementById('desc-benefit-2')
        var ben3 = document.getElementById('benefit-3')
        var descben3 = document.getElementById('desc-benefit-3')
        var ben4 = document.getElementById('benefit-4')
        var descben4 = document.getElementById('desc-benefit-4')
        var ben5 = document.getElementById('benefit-5')
        var descben5 = document.getElementById('desc-benefit-5')
        var ben6 = document.getElementById('benefit-6')
        var descben6 = document.getElementById('desc-benefit-6')

        ben1.className = 'topic'
        descben1.style.display = 'none'
        ben2.className = 'topic'
        descben2.style.display = 'none'
        ben3.className = 'topic'
        descben3.style.display = 'none'
        ben4.className = 'topic'
        descben4.style.display = 'none'
        ben5.className = 'topic'
        descben5.style.display = 'none'
        ben6.className = 'topic'
        descben6.style.display = 'none'

        if (num == 1) {
            ben1.className = 'topic active'
            descben1.style.display = ''
        }
        if (num == 2) {
            ben2.className = 'topic active'
            descben2.style.display = ''
        }
        if (num == 3) {
            ben3.className = 'topic active'
            descben3.style.display = ''
        }
        if (num == 4) {
            ben4.className = 'topic active'
            descben4.style.display = ''
        }
        if (num == 5) {
            ben5.className = 'topic active'
            descben5.style.display = ''
        }
        if (num == 6) {
            ben6.className = 'topic active'
            descben6.style.display = ''
        }
    }
</script>
